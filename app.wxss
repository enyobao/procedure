/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  /*align-items: center;*/
  justify-content: space-between;
  /*padding: 200rpx 0;*/
  box-sizing: border-box;
  font-size: 30rpx;
  color: #fff;
  background-color: #eee;
} 
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.clearfix:after{
  display: block;
  content: "";
  clear: both;
}
.clearfix{
  zoom:1;
}
